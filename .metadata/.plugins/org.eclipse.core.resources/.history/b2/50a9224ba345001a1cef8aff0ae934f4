package msnpro;

public class ArrayDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int nums[]= new int[4];
		nums[0]= 10;
		nums[1]=200;
		nums[2]=3000;
		nums[3]=40000;
		int[] vals= new int[5];
		
		
		for(int i=0; i<nums.length; i++) {
			System.out.println("The value of array at index "+ i +" is " + nums[i]);	
		}
		
	}

}
